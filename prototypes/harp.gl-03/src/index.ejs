<!DOCTYPE html>
<html>

<head>
    <title><%= title %></title>
    <% files.css.forEach(function (src) { %>
        <link rel="stylesheet" href="<%= src %>">
    <% }) %>
    <% files.js.forEach(function (src) { %>
        <% if (src.indexOf('head') !== -1) { %>
            <script src="<%= src %>"></script>
        <% } %>
    <% }) %>
</head>

<body>
<div id="app"></div>
<% files.js.forEach(function (src) { %>
    <% if (src.indexOf('head') === -1) { %>
        <script src="<%= src %>"></script>
    <% } %>
<% }) %>
</body>

</html>